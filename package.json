{
  "name": "giz-code",
  "version": "0.1.0",
  "description": "AI-powered interactive documentation and learning platform for any codebase",
  "private": false,
  "bin": {
    "giz-code": "./bin/cli.js"
  },
  "scripts": {
    "dev": "next dev -p 6001",
    "dev:ws": "WS_PORT=6002 npx tsx src/ws/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:ws\"",
    "build": "next build",
    "start": "next start -p 7001",
    "start:ws": "WS_PORT=7002 node --loader tsx src/ws/index.ts",
    "start:all": "concurrently \"npm run start\" \"npm run start:ws\"",
    "serve": "node bin/cli.js serve"
  },
  "keywords": [
    "documentation",
    "ai",
    "code-explorer",
    "developer-tools",
    "repository",
    "tutorial"
  ],
  "author": "GizAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GizAI/code.git"
  },
